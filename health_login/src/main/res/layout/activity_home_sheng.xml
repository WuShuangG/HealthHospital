<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:layout_marginTop="54dp"
        android:id="@+id/home_wanshan"
        android:textSize="16sp"
        android:textColor="#3087ea"
        android:text="完善信息"
        android:layout_marginLeft="30dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints"/>
    <TextView
        android:layout_marginLeft="30dp"
        android:id="@+id/home_shenhetontguo"
        android:textSize="16sp"
        android:textColor="#3087ea"
        android:layout_marginTop="15dp"
        android:text="审核通过即可成功入驻"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints"/>

    <TextView
        android:textColor="#666666"
        android:textSize="14sp"
        android:text="您的邮箱账号"
        android:id="@+id/home_you"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="30dp"
        tools:ignore="MissingConstraints"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:orientation="horizontal"
        >
    <EditText
        android:textSize="12sp"
        android:textColorHint="#666666"
        android:id="@+id/home_et_apply_name"
        android:paddingLeft="14dp"
        android:background="@drawable/realname"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_weight="1"
        tools:ignore="MissingConstraints"/>
        <Button
            android:textColor="#FAF6F6"
            android:background="@drawable/yanse"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/home_but_verificationcode"
            android:text="获取验证码"
            />

    </LinearLayout>
    <TextView
        android:textColor="#666666"
        android:textSize="14sp"
        android:text="请输入验证码"
        android:id="@+id/home_ninsuozaideyiyuan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="30dp"
        tools:ignore="MissingConstraints"/>
    <EditText
        android:id="@+id/home_et_apply_hospital"
        android:textSize="12sp"
        android:textColorHint="#666666"
        android:paddingLeft="14dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="30dp"
        android:background="@drawable/realname"
        android:layout_width="match_parent"
        android:layout_marginRight="30dp"
        android:layout_height="40dp"
        tools:ignore="MissingConstraints"/>
    <TextView
        android:textColor="#666666"
        android:textSize="14sp"
        android:text="请输入密码"
        android:id="@+id/home_ninsuozaikeshi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="30dp"
        tools:ignore="MissingConstraints"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:background="@drawable/realname"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        >
    <EditText
        android:paddingTop="0dp"
        android:id="@+id/home_et_apply_department"
        android:textSize="12sp"
        android:textColorHint="#666666"
        android:padding="14dp"
        android:background="@null"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:inputType="textPassword"
        style="?android:attr/textViewStyle"
        android:layout_weight="1"
        tools:ignore="MissingConstraints"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/home_sheng_mm_eye"
        android:layout_gravity="center"
        android:layout_marginRight="10dp"
        android:background="@drawable/eye"
        />
    </LinearLayout>
    <TextView
        android:textColor="#666666"
        android:textSize="14sp"
        android:text="请再次输入密码"
        android:id="@+id/home_nindezhicheng"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="30dp"
        tools:ignore="MissingConstraints"/>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:orientation="horizontal"
    android:background="@drawable/realname"
    android:layout_marginLeft="30dp"
    android:layout_marginTop="12dp"
    android:layout_marginRight="30dp"
    >
    <EditText
        android:id="@+id/home_et_apply_title"
        android:background="@null"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        style="?android:attr/textViewStyle"
        android:layout_weight="1"
        android:textColorHint="#666666"
        android:inputType="textPassword"
        android:padding="14dp"
        android:textSize="12sp"
        tools:ignore="MissingConstraints" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/home_sheng_zaici_eye"
        android:layout_gravity="center"
        android:layout_marginRight="10dp"
        android:background="@drawable/eye"
        />
</LinearLayout>
    <Button
        android:id="@+id/hone_btn_applyo_next"
        android:textSize="16sp"
        android:textColor="#fff"
        android:text="下一步"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="70dp"
        android:layout_marginRight="30dp"
        android:background="@drawable/btn_login"
        />
</LinearLayout>